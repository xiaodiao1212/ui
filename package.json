{
  "name": "ui",
  "version": "0.0.1",
  "description": "The most developer-friendly mobile component library",
  "main": "example/build/index.umd.js",
  "module": "example/build/index.js",
  "files": [
    "example/build"
  ],
  "types": "example/build/index.d.ts",
  "scripts": {
    "dev": "concurrently --kill-others \"rollup -wc rollup.config.js\" \"cd example && npm start\"",
    "build:dev": "cross-env NODE_ENV=development rollup -c rollup.config.js",
    "build:prod": "cross-env NODE_ENV=production && npm run lint && npm run test && rollup -c rollup.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "lint": "eslint --fix --ext .ts,.tsx src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fogcity/ui.git"
  },
  "keywords": [
    "emotion",
    "mobile",
    "css-in-ts",
    "rollup",
    "react 18"
  ],
  "author": {
    "name": "hyxs"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fogcity/ui/issues"
  },
  "size-limit": [
    {
      "path": "example/build/index.js",
      "limit": "15 KB"
    }
  ],
  "homepage": "https://github.com/fogcity/ui#readme",
  "devDependencies": {
    "@emotion/eslint-plugin": "^11.7.0",
    "@emotion/react": "^11.7.1",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@size-limit/preset-small-lib": "^5.0.5",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^13.0.0",
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jss-cli": "^6.0.2",
    "prettier": "^2.5.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rollup": "^2.63.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.3",
    "size-limit": "^5.0.5",
    "ts-jest": "^26.5.6",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "@emotion/react": ">=11.5.0",
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  },
  "dependencies": {
    "polished": "^4.1.4"
  }
}
