{
  "name": "ui",
  "version": "0.0.1",
  "description": "The component design that a good react mobile application should have",
  "main": "example/build/index.js",
  "module": "example/build/index.js",
  "files": [
    "example/build"
  ],
  "typings": "example/build/index.d.ts",
  "scripts": {
    "dev": "concurrently --kill-others \"rollup -wc rollup.config.js\" \"cd example && npm start\"",
    "build:dev": "cross-env NODE_ENV=development rollup -c rollup.config.js",
    "build:prod": "cross-env NODE_ENV=production && npm run lint && npm run test && rollup -c rollup.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "lint": "eslint --fix --ext .ts,.tsx src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fogcity/ui.git"
  },
  "keywords": [
    "vanilla-extract",
    "sprinkles",
    "css-in-js",
    "css-in-ts",
    "css",
    "webpack",
    "react"
  ],
  "author": {
    "name": "hyxs"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fogcity/ui/issues"
  },
  "size-limit": [
    {
      "path": "example/build/index.js",
      "limit": "15 KB"
    }
  ],
  "homepage": "https://github.com/fogcity/ui#readme",
  "devDependencies": {
    "@emotion/eslint-plugin": "^11.7.0",
    "@emotion/react": "^11.5.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@size-limit/preset-small-lib": "^5.0.1",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "@types/jest": "^26.0.7",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "classnames": "^2.2.6",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "jest": "^26.1.0",
    "jss-cli": "^6.0.2",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.53.3",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.3",
    "size-limit": "^5.0.1",
    "ts-jest": "^26.1.4",
    "tslib": "^2.3.0",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "@emotion/react": ">=11.5.0",
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  },
  "dependencies": {
    "clsx": "^1.1.1"
  }
}
